<resources>
    <!---->
    <style name="CheckBoxGreen" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/checkbox_green</item>
        <item name="android:layout_width">@dimen/dimen_checkbox_height</item>
        <item name="android:layout_height">@dimen/dimen_checkbox_height</item>
    </style>

    <style name="CheckBoxSelect" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/checkbox_select</item>
        <item name="android:layout_width">@dimen/dimen_checkbox_height</item>
        <item name="android:layout_height">@dimen/dimen_checkbox_height</item>
    </style>

    <style name="ListItemLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@drawable/bg_layout_white</item>
        <item name="android:paddingBottom">@dimen/dimen_layout_padding_top</item>
        <item name="android:paddingTop">@dimen/dimen_layout_padding_top</item>
        <item name="android:paddingLeft">@dimen/dimen_layout_padding_left</item>
        <item name="android:paddingRight">@dimen/dimen_layout_padding_left</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="WhiteIconLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/bg_layout_white</item>
        <item name="android:paddingBottom">@dimen/dimen_layout_padding_top</item>
        <item name="android:paddingTop">@dimen/dimen_layout_padding_top</item>
        <item name="android:paddingLeft">@dimen/dimen_layout_padding_top</item>
        <item name="android:paddingRight">@dimen/dimen_layout_padding_top</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="WhiteLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingBottom">@dimen/dimen_txt_padding_top</item>
        <item name="android:paddingTop">@dimen/dimen_txt_padding_top</item>
        <item name="android:paddingRight">@dimen/dimen_layout_padding_left</item>
        <item name="android:paddingLeft">@dimen/dimen_layout_padding_left</item>
        <item name="android:background">@drawable/bg_layout_white</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="IndentLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingBottom">@dimen/dimen_txt_padding_left</item>
        <item name="android:paddingTop">@dimen/dimen_txt_padding_left</item>
        <item name="android:paddingRight">@dimen/dimen_layout_indent</item>
        <item name="android:paddingLeft">@dimen/dimen_layout_indent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="CustomLabelTextForEdit">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/dimen_layout_padding_top</item>
        <item name="android:textSize">@dimen/dimen_font_16</item>
        <item name="android:textColor">@color/color_gray_font</item>
    </style>

    <style name="CustomEditText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/bg_input</item>
        <item name="android:paddingBottom">@dimen/dimen_txt_padding_top</item>
        <item name="android:paddingTop">@dimen/dimen_txt_padding_top</item>
        <item name="android:paddingLeft">@dimen/dimen_txt_padding_left</item>
        <item name="android:paddingRight">@dimen/dimen_txt_padding_left</item>
        <item name="android:layout_marginTop">@dimen/dimen_layout_padding_top</item>
        <item name="android:textSize">@dimen/dimen_font_18</item>
        <item name="android:textColor">@color/color_black_font</item>
        <item name="android:gravity">top</item>
    </style>

    <style name="CustomLabelText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/dimen_layout_padding_top</item>
        <item name="android:layout_marginLeft">@dimen/dimen_layout_padding_left</item>
        <item name="android:layout_marginRight">@dimen/dimen_layout_padding_left</item>
        <item name="android:textSize">@dimen/dimen_font_16</item>
        <item name="android:textColor">@color/color_gray_font</item>
    </style>

    <style name="CustomViewText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/dimen_layout_padding_top</item>
        <item name="android:layout_marginBottom">@dimen/dimen_layout_padding_top</item>
        <item name="android:layout_marginLeft">@dimen/dimen_layout_padding_left</item>
        <item name="android:layout_marginRight">@dimen/dimen_layout_padding_left</item>
        <item name="android:textSize">@dimen/dimen_font_18</item>
        <item name="android:textColor">@color/color_black_font</item>
        <item name="android:gravity">top</item>
    </style>

    <style name="NumViewText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">@dimen/dimen_sub_title_top</item>
        <item name="android:textSize">@dimen/dimen_font_14</item>
        <item name="android:textColor">@color/color_white</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/circle_red_message</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="LineRow">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dimen_divider_height</item>
        <item name="android:background">@drawable/bg_list_item</item>
    </style>

    <style name="LineCell">
        <item name="android:layout_width">@dimen/dimen_divider_height</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/color_line</item>
    </style>
    <!--字体样式-->
    <style name="GrayTxt12">
        <item name="android:textColor">@color/color_gray_font</item>
        <item name="android:textSize">@dimen/dimen_font_12</item>
    </style>

    <style name="GrayTxt14">
        <item name="android:textColor">@color/color_gray_font</item>
